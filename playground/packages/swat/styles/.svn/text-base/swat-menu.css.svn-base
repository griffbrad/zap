div.yuimenu {
	background:#fff url(../images/swat-frame-background.png) no-repeat scroll right top;
	border-color: #dcceb2;
}

div.yuimenubar {
	background: #f6f2ea url(../images/swat-toolbar-background.png) top left repeat-x;
	border: 0;
	border-bottom: 1px solid #efe8da;
	margin: -16px -16px 0 -16px;
	list-style-type: none;
	padding: 0;
}

div.yuimenu h6,
div.yuimenubar h6 {
	color: #837352;
	padding: 5px 8px;
	border: 0;
	text-align: left;
}

div.yuimenu li,
div.yuimenubar li {
	font-size: 100%;
}

div.yuimenu li a,
div.yuimenubar li a,
div.yuimenu li span,
div.yuimenubar li span {
	color: #3f5ace;
	outline-color: #efe8da;
	text-decoration: none;
}

a.swat-menu-item:hover,
.yuimenubaritem-hassubmenu-selected {
	background-color: #f3efe5;
}

div.yuimenu li.selected a.selected,
div.yuimenubar li.selected a.selected {
	color: #204a87;
	text-decoration: none;
	outline-color: #efe8da;
}

div.yuimenu li.selected,
div.yuimenubar li.selected,
div.yuimenu li.selected a.selected,
div.yuimenubar li.selected a.selected {
	background: #f3efe5;
}

div.yuimenu li.yuimenuitem,
div.yuimenubar li.yuimenubaritem,
div.yuimenubar li.yuimenubaritem.first-of-type {
	padding: 0;
	border: 0;
}

div.yuimenu .yuimenuitemlabel,
div.yuimenu .yuimenubaritemlabel {
	padding: 5px 8px;
}

.yuimenubar .yuimenu {
	border: 1px solid #dcceb2;
}


div.yuimenubar li.yuimenubaritem,
div.yuimenubar li.yuimenubaritem.first-of-type {
	border-color: #f0eade;
	border-style: solid;
	border-width: 0 1px 0 0;
}

div.yuimenubar ul {
	padding: 5px 0;
}

div.yuimenu ul {
	padding: 0;
}


/* Stock types */

div.yuimenubar .swat-menu-item-delete:link,
div.yuimenubar .swat-menu-item-delete:visited {
	background-image: url(../images/swat-tool-link-delete.png);
	background-position: 6px center;
	background-repeat: no-repeat;
	color: #c00;
	padding-left: 28px;
}

div.yuimenubar li.selected a.selected.swat-menu-item-delete {
	background: #f3efe5 url(../images/swat-tool-link-delete.png) 6px center no-repeat;
}

div.yuimenu .yuimenubaritemlabel-hassubmenu {
	background-image: url(../images/swat-menubar-hassubmenu.png);
	background-position: 100% 50%;
	background-repeat: no-repeat;
	padding-right: 24px;
}

div.yuimenu .yuimenuitemlabel-hassubmenu {
	background-image: url(../images/swat-menu-hassubmenu.png);
	background-position: 100% 50%;
	background-repeat: no-repeat;
	padding-right: 24px;
}

div.yuimenubar div.yuimenu li.hassubmenu,
div.yuimenubar div.yuimenu li.hashelptext {
}
